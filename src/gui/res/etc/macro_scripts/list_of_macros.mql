MACRO PGAPFixes_WGS_003 "Convert protein name to protein description (prefix separated by semicolon) where protein name equals 'CBS' (case-sensitive)"
VAR
    dest = "data.prot.desc"
    cap_change = "none"
    strip_name = false
    existing_text = "ePrepend"
    delimiter = ";"
    match = "CBS"
    case_sensitive = true
FOR EACH Protein
DO
    src = Resolve("data.prot.name") WHERE EQUALS(src, match, case_sensitive);
    ConvertStringQual(src, dest, cap_change, strip_name, existing_text, delimiter);
DONE
