MACRO Convert_Protein100_01 "Copy protein name to CDS comment(append separated by semicolon) where protein name is longer than 100 characters"
VAR
    cap_change = "none"
    existing_text = "eAppend"
    delimiter = ";"
FOR EACH Cdregion
DO
     o = RELATED_FEATURE("protein", "data.prot.name") WHERE STRLENGTH(o) > 100;
     ParseStringQual(o, "comment", cap_change, existing_text, delimiter);
DONE
------------------------------------------------------------------------------
MACRO Convert_Protein100_02 "Parse text just after ',' from protein name to protein name (overwrite existing text) where protein name is longer than 100 characters"
VAR
    include_left = false
    include_right = false
    case_sensitive = false
    whole_word = false
    rmv_from_parsed = true
    rmv_left = true
    cap_change = "none"
    existing_text = "eAppend"
    delimiter = ";"
FOR EACH Protein
WHERE STRLENGTH("data.prot.name") > 100
DO
     o = Resolve("data.prot.name") WHERE STRLENGTH(o) > 100;
     text = ParsedText(o, ",", include_left, "", include_right, case_sensitive, whole_word, rmv_from_parsed, rmv_left);
     UpdatemRNAProduct();
DONE
